@model bcpp.Models.uzivatel

@{
    ViewBag.Title = "Peněženka";
}

@using (var f = Html.Bootstrap().Begin(new Form().Type(FormType.Horizontal)))
{
            
    @Html.HiddenFor(model => model.uzivatel_id)
            
    @f.ControlGroup().TextBoxFor(m => m.penezenka).Label().LabelText("Peněženka")
    <p>
    @f.ControlGroup().CustomControls(Html.Bootstrap().SubmitButton().Text("Upravit"))
    </p>
}


    

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
